{
    "manifest_version": 2,
    "name": "Screenshot Enhancer",
    "version": "1.0",
    "description": "Capture and enhance screenshots",
    "permissions": ["activeTab","<all_urls>"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}